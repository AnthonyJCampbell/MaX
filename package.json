  "scripts": {
    "react-start": "cross-env EXTEND_ESLINT=true react-scripts start",
    "react-build": "cross-env EXTEND_ESLINT=true react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder -c.extraMetadata.main=build/node-server/node-server.js",
    "electron-build-win": "electron-builder --win -c.extraMetadata.main=build/node-server/node-server.js",
    "electron-build-mac": "electron-builder --mac -c.extraMetadata.main=build/node-server/node-server.js",
    "release": "npm run react-build && electron-builder --publish=always",
    "bundle-node-server": "webpack --config node-server/webpack.prod.js",
    "bundle-node-server-watch": "webpack --config node-server/webpack.dev.js --watch --no-stats",
    "build": "npm run _check-install && cross-env INLINE_RUNTIME_CHUNK=false npm run react-build && npm run bundle-node-server && npm run electron-build",
    "build-win": "npm run _check-install && cross-env INLINE_RUNTIME_CHUNK=false npm run react-build && npm run bundle-node-server && npm run electron-build-win",
    "build-mac": "npm run _check-install && cross-env INLINE_RUNTIME_CHUNK=false npm run react-build && npm run bundle-node-server && npm run electron-build-mac",
    "start": "npm run _check-install && concurrently \"npm run bundle-node-server-watch\" \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "debug": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron --inspect=5058 .\"",
    "format": "prettier \"./{src,public,node-server,test}/**/*.{js,jsx,ts,tsx}\" --write",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0 & npx ts-node --project tsconfig.base.json tools/linting/lint-package-lock.ts",
    "preinstall": "npm ls link-module-alias && npx link-module-alias clean || echo '! link-module-alias not installed - carry on with install'",
    "postinstall": " npx link-module-alias",
    "test": "npm run _check-install && concurrently \"cross-env BROWSER=none PORT=3001 npm run react-start\" \"jest test --runInBand\" --kill-others -s first",
    "test-all-mac": "npm run _check-install && jest test --runInBand",
    "test-electron-fv:pre-start": "npm run _check-install && npm run bundle-node-server && cross-env BROWSER=none PORT=3001 npm run react-start",
    "test-electron-fv:fv-command": "jest test/electron-fv/ --runInBand --forceExit",
    "test-electron-fv": "start-server-and-test test-electron-fv:pre-start http://localhost:3001 test-electron-fv:fv-command",
    "test-electron-fv-mac": "jest test/electron-fv/ --runInBand",
    "test-node-ut": "npm run _check-install && jest test/node-ut/ --runInBand --forceExit",
    "test-react-ut": "npm run _check-install && jest test/react-ut/",
    "test-react-ut-log": "npm run _check-install && jest test/react-ut/ --verbose > ./test/react-ut/react-uts-report.log",
    "build-storybook": "cross-env EXTEND_ESLINT=true build-storybook -s public && npx sb extract",
    "storybook": "cross-env EXTEND_ESLINT=true start-storybook -p 6006 -s public",
    "backstop:pre-test": "rm -fr backstop/output && rm -fr storybook-static && npm run build-storybook",
    "backstop:test": "npm run backstop:pre-test && backstop test --config=backstop.config.js",
    "backstop:test-local": "rm -fr backstop/output && cross-env LOCAL=true backstop test --config=backstop.config.js",
    "backstop:test-all": "npm run backstop:pre-test && cross-env ALL_RESOLUTIONS=true backstop test --config=backstop.config.js",
    "backstop:approve": "backstop approve --config=backstop.config.js",
    "backstop:approve-local": "cross-env LOCAL=true backstop approve --config=backstop.config.js",
    "add-package": "node tools/cli/add-package.js",
    "_check-install": "node tools/cli/check-install.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "projects": [
      "test/node-ut/node.jest.config.js",
      "test/react-ut/react.jest.config.js",
      "test/electron-fv/spectron.jest.config.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "Electron UC app",
  "author": "Metaswitch",
  "build": {
    "appId": "max_uc_react",
    "copyright": "Copyright 2021 Metaswitch Networks",
    "win": {
      "target": "zip",
      "icon": "src/assets/shared/icon.ico"
    },
    "mac": {
      "icon": "src/assets/shared/icon.icns",
      "target": "dir"
    },
    "files": [
      "./node-server/node-server.js"
    ]
  },
  "main": "build/node-server/node-server.js",
  "homepage": "./",
  "_moduleAliases": {
    "root": ".",
    "src": "./src",
    "node-server": "./node-server",
    "shared": "./src/shared",
    "public": "./public",
    "test": "./test",
    "assets": "./src/assets",
    "store": "./src/store",
    "reducers": "./src/store/reducers",
    "action-creators": "./src/store/action-creators",
    "selectors": "./src/store/selectors",
    "components": "./src/components"
  },
  "//": "When adding an alias to the list, run npm install to create the shortcuts"
